{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": "./src/",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "paths": {
      "@views/*": [
        "views/*"
      ],
      "@models/*": [
        "models/*"
      ],
      "@controllers/*": [
        "controllers/*"
      ],
      "@layouts/*": [
        "views/layouts/*"
      ],
      "@includes/*": [
        "views/includes/*"
      ],
      "@hooks/*": [
        "controllers/hooks/*"
      ],
      "@components/*": [
        "views/components/*"
      ],
      "@db/*": [
        "models/data/db/*"
      ],
      "@pages/*": [
        "models/data/pages/*"
      ],
      "@services/*": [
        "controllers/services/*"
      ],
      "@styles/*": [
        "views/styles/*"
      ],
      "@libs/*": [
        "views/libs/*"
      ],
      "@configs/*": [
        "models/configs/*"
      ],
      "@typings/*": [
        "models/typings/*"
      ],
      "@utils/*": [
        "controllers/utils/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}